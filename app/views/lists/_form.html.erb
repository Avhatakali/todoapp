<%= form_with(model: @list, local: true) do |form| %>

    <p>
      <%= form.label :Title %>  <br>
      <%= form.text_field :name %>
    </p>

     <p>
       <%= form.label :Description %>  <br>
       <%= form.text_field :description %>
     </p>

      <p>
          <%= form.submit %>
      </p>

<% end %>
